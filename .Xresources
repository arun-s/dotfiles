Xft*dpi:               96
Xft*antialias:          true
Xft*hinting:            full

URxvt.urgentOnBell: off
URxvt.visualBell: off
URxvt.iso14755: off
URxvt.iso14755_52: off
!URxvt.meta8: true
URxvt.depth: 32
URxvt.iconFile: /usr/share/icons/Humanity/apps/128/utilities-terminal.svg

URxvt.internalBorder: 0
URxvt.externalBorder: 0

URxvt.scrollBar: off
URxvt.saveLines: 1024
URxvt.scrollTtyOutput:   false
URxvt.scrollWithBuffer:  true
URxvt.scrollTtyKeypress: true
URxvt*geometry: 85x50

URxvt.inheritPixmap: false
URxvt.fading: 0

URxvt.font: xft:Inconsolata for Powerline:pixelsize=12:antialias=true:hinting=true,xft:DejaVu Sans Mono:pixelsize=12:antialias=true:hinting=true
URxvt.boldFont: xft:Inconsolata for Powerline:bold:pixelsize=12:antialias=true:hinting=true,xft:DejaVu Sans Mono:bold:pixelsize=12:antialias=true:hinting=true
URxvt.italicFont: xft:Inconsolata for Powerline:italic:pixelsize=12:antialias=true:hinting=true,xft:DejaVu Sans Mono:italic:pixelsize=12:antialias=true:hinting=true
URxvt.boldItalicFont: xft:Inconsolata for Powerline:bold:italic:pixelsize=12:antialias=true:hinting=true,xft:DejaVu Sans Mono:bold:italic:pixelsize=12:antialias=true:hinting=true
URxvt.letterSpace: -2


URxvt.url-select.button:   1
URxvt.url-select.launcher: x-www-browser

URxvt.clipboard.copycmd:  xsel --input  --clipboard
URxvt.clipboard.pastecmd: xsel --output --clipboard

URxvt.keysym.Control-Shift-C: perl:clipboard:copy
URxvt.keysym.Control-Shift-V: perl:clipboard:paste

URxvt.background: #121212
URxvt.foreground: #dddddd

